{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": [ "package.json", "README.md", "js"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs",
        "template":"./node_modules/minami"
    },
    "templates": {
        "monospaceLinks": true,
        "useLongnameInNav": false,
        "showInheritedInNav": true
    },
    "plugins": [
        "plugins/markdown"
    ]
}